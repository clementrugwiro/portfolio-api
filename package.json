{
  "name": "port-api",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "build": "babel ./src/index.js --out-file ./src/index-compiled.js",
    "start": "cross-env NODE_ENV=development babel-node ./src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec npm start",
    "test": "cross-env NODE_ENV=test nyc mocha --recursive --require @babel/polyfill --require @babel/register ./src/test/*.js --timeout 500000000 --exit",
    "cover": "cross-env NODE_ENV=test nyc mocha --recursive --exit ",
    "make-badges": "cross-env NODE_ENV=test istanbul-badges-readme",
    "make-badges-silent": "istanbul-badges-readme --silent",
    "make-badges-custom-dir": "istanbul-badges-readme --coverageDir='./my-custom-coverage'"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "reporter": [
      "json-summary",
      "text"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "babel-cli": "^6.26.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "chai-http": "^4.3.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "joi": "^13.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "istanbul-badges-readme": "^1.8.1",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "supertest": "^6.2.2"
  },
  "engines": {
    "node": "16.13.0"
  }
}
